    <!-- Scientific Calculator -->
    <h1>Scientific Calculator</h1>
    <div class="scientific-calculator">
        <input type="text" id="display" readonly>
        <div class="buttons">
            <button class="scientific" onclick="scientificCalculate('sin')">sin</button>
            <button class="scientific" onclick="scientificCalculate('cos')">cos</button>
            <button class="scientific" onclick="scientificCalculate('tan')">tan</button>
            <button class="scientific" onclick="scientificCalculate('cot')">cot</button>
            
            <button class="scientific" onclick="scientificCalculate('sqrt')">âˆš</button>
            <button class="scientific" onclick="scientificCalculate('log')">log</button>
            <button class="scientific" onclick="scientificCalculate('ln')">ln</button>
            <button class="scientific" onclick="scientificCalculate('^')">^</button>
            
            <button onclick="appendToDisplay('7')">7</button>
            <button onclick="appendToDisplay('8')">8</button>
            <button onclick="appendToDisplay('9')">9</button>
            <button class="operator" onclick="appendToDisplay('/')">/</button>
            
            <button onclick="appendToDisplay('4')">4</button>
            <button onclick="appendToDisplay('5')">5</button>
            <button onclick="appendToDisplay('6')">6</button>
            <button class="operator" onclick="appendToDisplay('*')">*</button>
            
            <button onclick="appendToDisplay('1')">1</button>
            <button onclick="appendToDisplay('2')">2</button>
            <button onclick="appendToDisplay('3')">3</button>
            <button class="operator" onclick="appendToDisplay('-')">-</button>
            
            <button onclick="appendToDisplay('0')">0</button>
            <button onclick="appendToDisplay('.')">.</button>
            <button onclick="calculate('=')">=</button>
            <button class="operator" onclick="appendToDisplay('+')">+</button>
            
            <button onclick="clearDisplay()">C</button>
            <button class="memory" onclick="memoryClear()">MC</button>
            <button class="memory" onclick="memoryRecall()">MR</button>
        </div>
    </div>
